{
  "extends": [
    "eslint:recommanded",
    "plugin:prettier/recommanded",
    "plugin:react/recommanded",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["prettier", "react"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": { "jsx": true }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error", {}, { "usePrettierrc": false }]
  }
}
